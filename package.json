{
  "name": "med-soc",
  "version": "1.0.0",
  "description": "A npm workspace for med-soc app",
  "author": "Anoop Benzier (HubSpire)",
  "private": true,
  "license": "ISC",
  "engineStrict": true,
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">=8.0.0 <9.0.0"
  },
  "scripts": {
    "nx": "nx",
    "nx:w": "nodemon --exec nx --ext ts  ",
    "postinstall": "husky install",
    "clean": "rimraf ./apps/**/dist ./libs/**/dist ./apps/**/coverage ./apps/**/node_modules ./libs/**/node_modules node_modules",
    "commit": "git-cz",
    "codegen:type": "graphql-codegen --config codegen.type.yaml -w",
    "codegen:sdk": "graphql-codegen --config codegen.sdk.yaml -w"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "{apps,libs}/**/src/**/*.ts": [
      "eslint {apps,libs}/**/src/*.ts --fix-dry-run",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^6.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/bcryptjs": "^2.4.4",
    "@types/cors": "^2.8.14",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/lodash": "^4.14.199",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.8.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "commitizen": "^4.3.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-customizable": "^7.0.0",
    "esbuild": "^0.19.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "nx": "^16.10.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "winston": "^3.11.0"
  }
}
