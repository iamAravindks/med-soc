{
  "name": "feed-service",
  "version": "1.0.0",
  "description": "Feed service for med-soc",
  "private": true,
  "scripts": {
    "test": "jest --testPathPattern=./dist --coverage --forceExit",
    "test:watch": "jest --testPathPattern=./src --watch",
    "postbuild": "node ../../scripts/copy-files.js",
    "build": "node ./esbuild.config.js",
    "apps:watch": "../../node_modules/nodemon/bin/nodemon.js",
    "serve": "ts-node ./src/app.ts",
    "start": "node ./dist/app.js",
    "docker:build": "docker build -t $ECR_REGISTRY/feed-service:$Environment -t $ECR_REGISTRY/feed-service:$IMAGE_TAG -f Dockerfile ../../",
    "docker:push": "docker push --all-tags $ECR_REGISTRY/feed-service"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@apollo/subgraph": "^2.5.5",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.0",
    "@graphql-tools/utils": "^10.0.6",
    "@hubspire/cache-directive": "^1.1.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.2",
    "mongodb-memory-server": "^9.0.0",
    "mongoose": "^7.6.0",
    "redis-memory-server": "^0.7.0"
  }
}
